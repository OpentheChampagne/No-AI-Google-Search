{
  "manifest_version": 3,
  "name": "No AI Google Search",
  "version": "0.1.3",
  "description": "Turn off AI Previews in Google Search",
  "background": {
    "service_worker": "bg.js"
  },
  "permissions": [
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "https://www.google.com/*"
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}